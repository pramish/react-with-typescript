  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "docker-build-development": "docker-compose -f docker-compose.dev.yml build",
    "docker-run-development": "docker-compose -f docker-compose.dev.yml up",
    "docker-build-production": "docker-compose -f docker-compose.prod.yml build",
    "docker-run-production": "docker run -p 80:80 --name react-app get-help-with-resume__client__production",
